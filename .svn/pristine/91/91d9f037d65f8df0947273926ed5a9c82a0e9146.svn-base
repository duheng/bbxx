page{
    height:100%;
}
.container{
    padding:20rpx;
    margin:0;
    width:100%;
    height:100%;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
.container-swiper{
    box-shadow: 0 1px 3px rgba(34, 25, 25, .4);
    -webkit-box-shadow: 0 1px 3px rgba(34, 25, 25, .4);
    border: 1px solid white;
    padding: 10rpx;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    overflow: hidden;
}
.container-swiper-image{
    width: 100%;
}
.container-title{
    width: 100%;
    line-height: 45rpx;
    font-size: 32rpx;
    margin: 20rpx 0;
    overflow: hidden;
    text-align: center;
}
.container-praise-item{
    width: 100%;
    overflow: hidden;
    margin: 20rpx 0;
}
.container-praise-image{
    height: 60rpx;
    width: 60rpx;
    float: left;
    margin: 5rpx;
    border-radius: 5rpx;
}
.container-praise-bt{
    height: 60rpx;
    line-height: 40rpx;
    float: left;
    margin: 5rpx;
    padding: 10rpx 20rpx 10rpx 60rpx;
    font-size: 32rpx;
    color: #ff6699;
    border-radius: 10rpx;
    border: 1rpx solid #ff6699;
    box-sizing: border-box;
}
.container-navigator{
    margin: 50rpx 2rpx;
    background-color: #ff6699!important;
    position:relative;
    display:block;
    padding-left:14px;
    padding-right:14px;
    box-sizing:border-box;
    font-size:36rpx;
    text-align:center;
    text-decoration:none;
    height:90rpx;
    line-height:90rpx;
    border-radius:10rpx;
    -webkit-tap-highlight-color:transparent;
    overflow:hidden;
    color:#fff;
}