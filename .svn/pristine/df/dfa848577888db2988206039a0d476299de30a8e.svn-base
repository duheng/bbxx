page{
    background: #fff;
}
.container{
    padding:0;
    margin:0;
    width:100%;
    height:100%;
}
.container-section{
    overflow: hidden;
    margin-bottom: 20rpx;
    padding-left: 30rpx;
}
.container-row{
    position: relative;
    line-height: 40rpx;
    font-size: 28rpx;
    color: #404040;
}
.container-title{
    height: 80rpx;
    line-height: 80rpx;
    font-size: 32rpx;
    text-align: center;
}

.bold{
    font-weight: bold;
}