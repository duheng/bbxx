page{
    height:100%;
}
.container{
    padding:20rpx;
    margin:0;
    width:100%;
    height:100%;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
.container-swiper{
    box-shadow: 0 1px 3px rgba(34, 25, 25, .4);
    -webkit-box-shadow: 0 1px 3px rgba(34, 25, 25, .4);
    border: 1px solid white;
    padding: 10rpx;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    height: 100%;
    overflow: hidden;
}
.container-swiper-image{
    width: 100%;
}
.container-swiper-title{
    width: 100%;
    line-height: 40rpx;
    font-size: 32rpx;
    margin: 20rpx 0;
    overflow: hidden;
    text-align: center;
}
