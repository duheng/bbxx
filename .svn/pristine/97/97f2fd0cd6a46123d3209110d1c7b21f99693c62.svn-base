<view class="container">
    <swiper class="container-swiper" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
        <block wx:for="{{items}}" wx:key="id">
            <swiper-item>
                <image bindtap="previewImage" data-url="{{item.src}}" src="{{item.src}}" mode="widthFix" class="container-swiper-image" />
                <view class="container-swiper-title">{{title}}</view>
            </swiper-item>
        </block>
    </swiper>
</view>
<import src="../../template/toast/index.wxml"/>
<template wx:if="{{toast.show}}" is="toast" data="{{...toast}}"/>